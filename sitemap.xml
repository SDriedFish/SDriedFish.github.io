layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<urlset>
    {% for post in site.posts %}
    <url>
        <loc>{{ site.url }}{{ post.url }}</loc>
        <changefreq>daily</changefreq>
        <priority>1</priority>
        <lastmod>{{ post.date }}</lastmod>        
    </url>
    {% endfor %}
</urlset>
